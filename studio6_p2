function remove_duplicates(lst){
    
    function checklst(item, lst){
        return !is_null(filter(x => x === item, lst));
    }
    
    function nlst (lst, final){
        return is_null(lst)
            ? final 
            : checklst(head(lst), final)
            ? nlst(tail(lst), final)
            : nlst(tail(lst), append(final, list(head(lst))));
    }
    
    const initial = null;
    return nlst(lst, initial);
    
    
}

remove_duplicates(list(1,2,3,2,3,5));